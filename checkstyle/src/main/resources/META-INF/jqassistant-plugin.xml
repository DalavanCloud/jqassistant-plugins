        <type>de.kontext_e.jqassistant.plugin.checkstyle.store.descriptor.CheckstyleDescriptor</type>
        <type>de.kontext_e.jqassistant.plugin.checkstyle.store.descriptor.ErrorDescriptor</type>
        <type>de.kontext_e.jqassistant.plugin.checkstyle.store.descriptor.FileDescriptor</type>
    </store>
    <scanner>
        <plugin>de.kontext_e.jqassistant.plugin.checkstyle.scanner.CheckstyleScannerPlugin</plugin>
