<jqa:jqassistant-rules xmlns:jqa="http://www.buschmais.com/jqassistant/core/analysis/rules/schema/v1.0">

    <constraint id="git:GitNodeMustExist">
        <description>For debugging purposes</description>
        <cypher><![CDATA[
                match
                    (g:Git)
                return
                    CASE count(g)
                    WHEN 0
                    THEN 'Error: Git not imported'
                    WHEN 1
                    THEN 'OK
                    ELSE 'Too much git root nodes' END as result
        ]]></cypher>
    </constraint>

    <group id="default">
<!--
        <includeConstraint refId="git:GitNodeMustExist"/>
-->
    </group>

</jqa:jqassistant-rules>
