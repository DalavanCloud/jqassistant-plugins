<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="FindBugs">
    <description>Provides a scanner for FindBugs XML result files.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.findbugs.store.descriptor.FindBugsDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.findbugs.store.descriptor.BugInstanceDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.findbugs.store.descriptor.SourceLineDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.findbugs.store.descriptor.BugInstanceClassDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.findbugs.store.descriptor.BugInstanceMethodDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.findbugs.store.descriptor.BugInstanceFieldDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.findbugs.scanner.FindBugsScannerPlugin</class>
    </scanner>
  <rules>
      <resource>findbugs.xml</resource>
  </rules>
</jqa-plugin:jqassistant-plugin>
