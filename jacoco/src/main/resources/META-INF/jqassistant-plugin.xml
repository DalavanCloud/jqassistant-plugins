<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0">
    <description>Provides a scanner for JaCoCo XML test coverage result files.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.JacocoDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.PackageDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.ClassDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.MethodDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.jacoco.store.descriptor.CounterDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.jacoco.scanner.JacocoScannerPlugin</class>
    </scanner>
  <rules>
      <resource>jacoco.xml</resource>
  </rules>

</jqa-plugin:jqassistant-plugin>
